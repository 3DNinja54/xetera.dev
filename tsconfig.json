{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "💅/*": ["src/styles/*"],
      "🛠️/*": ["src/scripts/*"],
      "🧱/*": ["src/components/*"],
      "@article/*": ["src/components/article*"],
      "🗺️/*": ["src/layouts/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
